{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "es5",
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "styles/theme.js",
    "pages/_document.js",
    "pages/index.tsx",
    "pages/_app.tsx"
, "services/footerService.js", "services/barraDeContatosService.js", "components/tituloGeralSemImagem.js"  ],
  "exclude": [
    "node_modules"
  ]
}
